#! /bin/zsh
#
# find all possible labels that can be used to search a machine.
#

emulate -L zsh -o pipe_fail -o err_return

tsh ls --format=json                      | \
jq -C '.[]
       | .metadata.labels + .spec.cmd_labels
       | keys
       | .[]'                             | \
sort                                      | \
uniq
